<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">
    <class name="ReportesIndicadores" table="reportes_indicadores" lazy="false" >

        <id name="idReporte"
            column="id_reporte"
            type="java.lang.Integer"
            unsaved-value="null" >
            <generator class="increment">
            </generator>
        </id>

        <property name="descripcion" column="descripcion"/>
        <property name="numReporte" column="num_reporte"/>


        <many-to-one name="idRegistroIndicadores" class="RegistroIndicadores" fetch="select">
            <column name="id_registro_indicadores" not-null="true" />
        </many-to-one>



    </class>
</hibernate-mapping>