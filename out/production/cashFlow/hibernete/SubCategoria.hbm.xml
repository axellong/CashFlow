<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
    <class name="SubCategoria" table="sub_categoria" lazy="false">

        <id name="idSubCategoria"
            column="id_sub_categoria"
            type="java.lang.Integer"
            unsaved-value="null" >
            <generator class="increment">
            </generator>
        </id>

        <property name="nombreSubCategoria" column="nombre_sub_categoria"/>

        <many-to-one name="id_Categoria" class="Categoria" fetch="select">
            <column name="id_categoria" not-null="true" />
        </many-to-one>


    </class>
</hibernate-mapping>