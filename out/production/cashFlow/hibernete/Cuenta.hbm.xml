<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
    <class name="Cuenta" table="cuenta" lazy="false">

        <id name="id_Cuenta"
            column="id_cuenta"
            type="java.lang.Integer"
            unsaved-value="null" >
            <generator class="increment">
            </generator>
        </id>

        <property name="cuenta" column="numero_cuenta"/>

        <set name="registroIndicadores" table="registro_indicadores" fetch="select">
            <key>
                <column name="id_cuenta" not-null="true">
                </column>
            </key>
            <one-to-many class="RegistroIndicadores"/>
        </set>


    </class>
</hibernate-mapping>
