<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
    <class name="RegistroIndicadores" table="registroIndicadores">

        <id name="idRegistroIndicadores"
            column="idRegistroIndicadores"
            type="java.lang.Integer"
            unsaved-value="null" >
            <generator class="native">
            </generator>
        </id>

        <property name="idClasificacion" column="idClasificacion"/>
        <property name="concepto" column="concepto"/>
        <property name="razonSocial" column="razonSocial"/>
        <property name="monto" column="monto"/>
        <property name="mes" column="mes"/>
        <property name="semana" column="semana"/>
        <property name="ano" column="ano"/>
        <property name="descripcion" column="descripcion"/>




    </class>
</hibernate-mapping>