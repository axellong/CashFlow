<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<AnchorPane id="base-category" prefHeight="480.0" prefWidth="550.0" scaleZ="-1.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controller.FlujoController">
    <stylesheets>
        <URL value="@../General.css"/>
        <URL value="@Flujo.css"/>
    </stylesheets>
    <children>
        <TableView id="table-base" fx:id="tableViewFlujo" layoutY="20.0" prefHeight="220.0" prefWidth="550.0">
            <columns>
                <TableColumn fx:id="colFecha" maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="Fecha"/>
                <TableColumn fx:id="colDescripcion" maxWidth="171.0" minWidth="171.0" prefWidth="171.0"
                             text="Descripción"/>
                <TableColumn fx:id="colCategoria" maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="Categoria"/>
                <TableColumn fx:id="colSubCategoria" maxWidth="138.0" minWidth="138.0" prefWidth="138.0"
                             text="SubCategoria"/>
            </columns>
        </TableView>
        <JFXCheckBox id="checkbox-base" fx:id="checkEntrada" layoutX="14.0" layoutY="254.0" text="Entrada">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXCheckBox>
        <JFXCheckBox id="checkbox-base" fx:id="checkSalida" layoutX="114.0" layoutY="254.0" text="Salida">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXCheckBox>
        <JFXComboBox id="box-style" fx:id="boxCategoria" focusColor="WHITE" labelFloat="true" layoutX="14.0"
                     layoutY="304.0" prefHeight="30.0" prefWidth="230.0" promptText="Categoria :"/>
        <JFXTextField id="input-label" fx:id="inputDescripcion" focusColor="WHITE" labelFloat="true" layoutX="14.0"
                      layoutY="358.0" prefHeight="30.0" prefWidth="230.0" promptText="Descripción :">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXTextField id="input-label" fx:id="inputCantidad" focusColor="WHITE" labelFloat="true" layoutX="14.0"
                      layoutY="410.0" prefHeight="30.0" prefWidth="150.0" promptText="Cantidad :">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXButton id="button-base" layoutX="344.0" layoutY="410.0" onMouseClicked="#MouseClickedSave" prefHeight="35.0"
                   prefWidth="180.0" text="Guardar" textFill="WHITE">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXButton>
        <DatePicker fx:id="datePickerFecha" layoutX="280.0" layoutY="304.0" prefHeight="30.0" prefWidth="200.0"/>
        <Label layoutX="280.0" layoutY="279.0" text="Fecha" textFill="WHITE">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
