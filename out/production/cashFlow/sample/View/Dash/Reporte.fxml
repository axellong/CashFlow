<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<AnchorPane id="base-panel" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ReporteController">
    <children>
        <AnchorPane id="pane" prefHeight="200.0" prefWidth="400.0">
            <children>
                <Label layoutX="29.0" layoutY="63.0" text="Fecha: " textFill="WHITE">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Label>
                <JFXButton id="button-base" layoutX="235.0" layoutY="146.0" onMouseClicked="#MouseClickedReporte"
                           prefHeight="30.0" prefWidth="150.0" text="Generar Reporte" textFill="WHITE">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </JFXButton>
                <HBox alignment="CENTER" layoutX="342.0" layoutY="14.0"/>
                <DatePicker fx:id="datePickerReporte" layoutX="29.0" layoutY="91.0"/>
                <JFXButton id="button-close" buttonType="RAISED" layoutX="371.0" layoutY="9.0" minHeight="-Infinity"
                           minWidth="-Infinity" onMouseClicked="#MouseClickedClose" prefHeight="12.0" prefWidth="12.0"
                           ripplerFill="#6b6b6b" text="Button"/>
            </children>
        </AnchorPane>
    </children>
    <stylesheets>
        <URL value="@Reporte.css"/>
        <URL value="@../General.css"/>
    </stylesheets>
</AnchorPane>
